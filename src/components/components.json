{
    "root": {
        "elements": {
            "Block": [
                ["div"], "block"
            ],
            "Box": [
                ["div"], "box"
            ],
            "Content": [
                ["dl", "ol", "ul", "p", "h1", "h2", "h3",
                    "h4", "h5", "h6", "em", "strong", "table", "th", "tr",
                    "td", "blockquote"
                ], "content", {
                    "olBulletType": [
                        "is-lower-alpha",
                        "is-upper-alpha",
                        "is-lower-roman",
                        "is-upper=roman"
